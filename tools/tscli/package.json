{
  "name": "@known-as-bmf/tscli",
  "description": "Typescript build & test CLI",
  "version": "1.0.0",
  "author": "Julien Avert",
  "bin": {
    "tscli": "./bin/run"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.3",
    "@oclif/parser": "3.8.5",
    "@oclif/plugin-help": "3.2.0",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "babel-jest": "26.5.2",
    "brotli-size": "4.0.0",
    "eslint": "7.10.0",
    "find-up": "5.0.0",
    "fs-extra": "9.0.1",
    "gzip-size": "5.1.1",
    "jest-config": "26.5.2",
    "jest": "26.5.2",
    "pretty-bytes": "5.4.1",
    "progress-estimator": "0.3.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-sourcemaps": "0.6.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup": "2.28.2",
    "superstruct": "0.10.12",
    "ts-jest": "26.4.1",
    "tslib": "2.0.2",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@jest/types": "26.5.2",
    "@known-as-bmf/eslint-config-bmf": "workspace:*",
    "@oclif/dev-cli": "1.22.2",
    "@types/eslint": "7.2.3",
    "@types/fs-extra": "9.0.1",
    "@types/node": "14.11.5",
    "@types/validate-npm-package-name": "3.0.0",
    "eslint": "7.10.0",
    "globby": "11.0.1",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "tscli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "rimraf dist && tsc -b && oclif-dev readme",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
